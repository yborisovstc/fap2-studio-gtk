<?xml version="1.0" ?>

<!-- Specification of FAP environment for empty root -->

<!DOCTYPE  node SYSTEM "/usr/share/grayb/conf/objspec.dtd"> 

<node id="Root" parent="Elem">
    <node id="Note" parent="Description">
	<cont val="This is model template for incapsulated system. 
	    Add required modules to [Modules] node and start creating model in [IRoot]"/>
    </node>
    <node id="Modules" parent="Elem">
	<move id="file:/usr/share/grayb/modules/syst.xml#"/>
	<move id="file:/usr/share/grayb/modules/capsula.xml#"/>
    </node>
    <node id="IRoot" parent="/Root/Modules/IncapsComps/Incaps"/>
</node> 

